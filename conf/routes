# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.HomeController.index
# An example controller showing how to use dependency injection
GET     /count                      controllers.CountController.count
# An example controller showing how to write asynchronous code
GET     /message                    controllers.AsyncController.message

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)



#GET     /tasks                      controllers.HomeController.tasks
GET     /help                      controllers.HomeController.help


GET     /PastCard                      controllers.SakihamaController.PastCard
GET     /sortOldCard                      controllers.SakihamaController.sortOldCard

GET     /PastCard_A                     controllers.SakihamaController.PastCard_A
GET     /PastCard_B                     controllers.SakihamaController.PastCard_B
GET     /PastCard_C                     controllers.SakihamaController.PastCard_C
GET     /PastCard_D                     controllers.SakihamaController.PastCard_D

GET     /PastCard_resultTop                      controllers.SakihamaController.PastCard_resultTop

GET     /PastCard_result_1                      controllers.SakihamaController.PastCard_result_1
GET     /PastCard_result_2                      controllers.SakihamaController.PastCard_result_2
GET     /PastCard_result_3                      controllers.SakihamaController.PastCard_result_3
GET     /PastCard_result_4                      controllers.SakihamaController.PastCard_result_4
